{
  "name": "arweave_transaction_viewer",
  "version": "0.1.0",
  "private": true,
  "dependencies": {
    "@emotion/react": "^11.9.0",
    "@emotion/styled": "^11.8.1",
    "@mui/material": "^5.8.2",
    "@testing-library/jest-dom": "^5.14.1",
    "@testing-library/react": "^13.0.0",
    "@testing-library/user-event": "^13.2.1",
    "arweave": "^1.10.23",
    "react": "^18.1.0",
    "react-dom": "^18.1.0",
    "react-relay": "^13.2.0",
    "react-scripts": "5.0.1",
    "relay-runtime": "^13.2.0",
    "web-vitals": "^2.1.0"
  },
  "scripts": {
    "start": "react-scripts start",
    "build": "yarn run relay && react-scripts build",
    "relay": "yarn run relay-compiler $@",
    "test": "react-scripts test",
    "eject": "react-scripts eject",
    "docker:build": "docker build -t arweave_viewer .",
    "docker:run": "docker run -d -p 3100:80 arweave_viewer",
    "docker:export": "docker save -o arweave_viewer.tar arweave_viewer"
  },
  "relay": {
    "src": "./src/",
    "schema": "./schema.graphql"
  },
  "eslintConfig": {
    "extends": [
      "react-app",
      "react-app/jest"
    ]
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "devDependencies": {
    "babel-plugin-relay": "^13.2.0",
    "relay-compiler": "^13.2.0"
  }
